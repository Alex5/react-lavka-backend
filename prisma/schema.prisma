generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Store {
  id Int @default(autoincrement()) @id
  address String @unique
  catalog_id Int @unique
  catalog Catalog @relation(fields: [catalog_id], references: [id])
}

model Catalog {
  id Int @default(autoincrement()) @id
  store_id Int @unique
  store Store?
}